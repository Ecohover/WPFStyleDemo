<Window x:Class="WPFDemo.StartPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFDemo"
        mc:Ignorable="d"
        Title="StartPage" Height="149.851" Width="438.333">
    <Grid>
        <Button Content="开启页面" HorizontalAlignment="Left" Margin="230,25,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <ComboBox x:Name="cbStyleName" HorizontalAlignment="Left" Margin="100,23,0,0" VerticalAlignment="Top" Width="120" ItemsSource="{Binding PageStyleList}" SelectedValuePath="Key" DisplayMemberPath="Value" SelectedIndex="1" SelectionChanged="cbStyleName_SelectionChanged"/>
        <Button x:Name="btn_Test" Content="Test" Visibility="Collapsed" HorizontalAlignment="Left" Margin="175.678,72.987,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btn_DockTest" Content="开启Dock页面" HorizontalAlignment="Left" Margin="230,49,0,0" VerticalAlignment="Top" Width="95" Click="btn_DockTest_Click"/>

    </Grid>
</Window>
